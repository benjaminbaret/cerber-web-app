{
  "name": "supabase-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prisma:dev:studio": "npx dotenv -e .env -- prisma studio",
    "prisma:dev:deploy": "prisma migrate deploy",
    "prisma:dev:refresh": "npx dotenv -e .env -- prisma migrate dev",
    "db:dev:rm": "docker-compose -f docker-compose.dev-test.yml rm -s -f -v dev-db",
    "db:dev:up": "docker-compose -f docker-compose.dev-test.yml up -d dev-db",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && sleep 1 && npm run prisma:dev:deploy",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^5.15.5",
    "@mui/styled-engine-sc": "^6.0.0-alpha.12",
    "@mui/x-charts": "^6.19.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.8.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.39.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "js-cookie": "^3.0.5",
    "mui-file-dropzone": "^4.0.2",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.21.3",
    "styled-components": "^6.1.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "prisma": "^5.8.1",
    "supabase": "^1.136.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
